--animation
function player_ani()
		if plyr.falling then
			plyr.s = 4
  else
   plyr.s = 1
  end

  if plyr.jumping then
   plyr.s = 4
  else
   plyr.s = 1
  end
  
  if plyr.running then
  	if time()-plyr.anim>.1 then
  		plyr.anim=time()
  		plyr.s+=1
  		if plyr.s>3 then
  			plyr.s=2
  		end
  	end
end
  	
end		
